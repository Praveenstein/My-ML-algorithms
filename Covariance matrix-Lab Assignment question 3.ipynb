   "source": [
    "import numpy as np\n",
    "x=np.random.randint(1,100,[150,4])\n",
    "def fun(a):\n",
    "    return np.mean(a)\n",
    "mean=np.apply_along_axis(fun,0,x)\n",
    "x_u=x-mean\n",
    "x_u_t=np.transpose(x_u)\n",
    "shape=x.shape[0]\n",
    "cov=(np.matmul(x_u_t,x_u))/shape\n",
    "print(cov)\n"
   ]
  }
